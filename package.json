{
  "name": "@zcbenz/cmake-binaries",
  "version": "1.3.17-2",
  "description": "cmake binaries for npm",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "os": [
    "win32",
    "linux",
    "darwin"
  ],
  "cpu": [
    "x64",
    "x32"
  ],
  "engines": {
    "node": ">=6.4.0"
  },
  "cmake_version_base": "3.17",
  "cmake_version_rev": "2",
  "scripts": {
    "preinstall": "node preinstall.js",
    "prepublishOnly": "node prepublish.js"
  },
  "bin": {
    "cmake": "bin/cmake",
    "cpack": "bin/cpack",
    "ctest": "bin/ctest",
    "ccmake": "bin/ccmake",
    "cmake-gui": "bin/cmake-gui",
    "cmcldeps": "bin/cmcldeps"
  },
  "author": "DayBr3ak <not-an-email-yet>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yue/cmake-binaries.git"
  },
  "homepage": "https://github.com/yue/cmake-binaries"
}
